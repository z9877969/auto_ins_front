{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "modules/*": ["modules/*"],
      "hooks/*": ["hooks/*"],
      "pages/*": ["pages/*"],
      "@redux/*": ["redux/*"],
      "shared/*": ["shared/*"],
      "helpers/*": ["helpers/*"],
      "context/*": ["context/*"],
      "services/*": ["services/*"],
      "@constants/*": ["constants/*"],
      "images/*": ["images/*"],
      "assets/*": ["assets/*"],
      "components/*": ["components/*"]
    }
  },
  "exclude": ["node_modules", "dist", ".vite"]
}
